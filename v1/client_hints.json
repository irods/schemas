{
    "id": "client_hints",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "strict_acls": {"type": "string", "enum": ["on", "off"]},
        "hash_scheme": {"type": "string", "enum": ["md5", "sha256"]},
        "match_hash_policy": {"type": "string", "enum": ["strict", "not_strict"]},
        "specific_queries": { 
            "type": "array", 
            "items": {
                "type": "string"
            }
        }
    },
    "required": ["strict_acls", "hash_scheme", "match_hash_policy", "specific_queries"]
}
